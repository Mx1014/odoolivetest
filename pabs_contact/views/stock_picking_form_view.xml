<odoo>
    <data>
        <record id="stock_picking_form_view_inherit_pabs_contact" model="ir.ui.view">
            <field name="name">stock.picking.form.view.inherit.pabs.contact</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <field name="partner_id" position="after">
                    <field name="x_address"/>
                    <field name="x_mobile"/>
                    <field name="x_city" invisible="1"/>
                    <field name="x_block" invisible="1"/>
                    <field name="x_zone" invisible="1"/>
                    <field name="x_road" invisible="1"/>
                    <field name="x_house" invisible="1"/>
                </field>
            </field>
        </record>
    </data>
</odoo>