<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="fleet_vehicle_view_form_inherit_pabs_logistics" model="ir.ui.view">
        <field name="name">fleet.vehicle.form.inherit.pabs.logistics</field>
        <field name="model">fleet.vehicle</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet//group//group[last()]" position="after">
                <group>
                    <field name="picking_type_id" />
                    <field name="capacity_qty" />  
                </group>
            </xpath>
        </field>
    </record>
</odoo>
